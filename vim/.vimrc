nnoremap <C-L> :nohlsearch\|redraw!<CR><C-L>
nnoremap <SPACE><SPACE>y :%y+<CR>
nnoremap <SPACE>sp :set paste!<CR>
nnoremap <SPACE>tn :tabnew 
set background=dark
set hlsearch
set ignorecase
set incsearch
set nowrap
set ruler
set smartindent
set tabstop=2 shiftwidth=2 expandtab
syntax on
vnoremap <SPACE>s :sort<CR>
vnoremap <SPACE>y "+y
